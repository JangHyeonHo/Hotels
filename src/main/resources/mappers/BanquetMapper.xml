<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="banquet">
	<cache />
	<insert id="Binsert" parameterType="Banquet">
		inser into BANQUET(BANQUET,BANQUET,BANQUET,BAN_MAX_PEOPLE,BAN_STORE_FILE_NAME) values(#{banName}, #{banPoundation} , #{banDescription}, #{banTime}, #{banMaxPeople}, #{banStoreFileName})
	</insert>
	
	
</mapper>