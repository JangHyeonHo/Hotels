<cache/>
<insert id="rinsert" parameterType="Restaurant">

 INSERT INTO RESTAURANT(RES_NAME,RES_CHAIR,RES_TABLE_COUNT,RES_DESCRIPTION,RES_STORE_FILE_NAME) 
